<AnchorPane id="AnchorPane" prefHeight="524.0" prefWidth="723.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="municipality.EventFXMLController">
   <children>
      <Label layoutX="17.0" text="Title">
