      </Label>
      <Label layoutX="15.0" layoutY="55.0" text="Type">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="17.0" layoutY="116.0" text="Location">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="171.0" text="Description">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextArea fx:id="descriptionArea" layoutX="14.0" layoutY="201.0" prefHeight="182.0" prefWidth="433.0" promptText="The Event wil start at 19:00. " />
